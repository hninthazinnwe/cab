<h3>Cab List</h3>
<% if @cabs.blank? %>
<p>There are not any cabs currently in the system.</p>

<% else %>
<b> <%= link_to "Download All", {:action => 'download'} %></b>

<ul id = "books">
   <% @cabs.each do |c| %>
   <li><%= link_to c.plate_number, {:action => 'show', :id => c.id} -%>
   <b> <%= link_to 'Edit', {:action => "edit",
   :id => c.id} %></b>
   <b> <%= link_to "Delete", {:action => 'delete', :id => c.id},
      :confirm => "Are you sure you want to delete this item?" %></b>
   </li>
   <% end %>
</ul>

<% end %>
<p><%= link_to "Add New", {:action => 'new' }%></p>